import math as m

def g7(x):
    return (7 - 4*m.log(x))/3

def PointFixe(g, xo, epsilon, Nitermax ):
    xold = xo
    n = 1
    xnew = g(xold)
    diff = xnew - xold
    xold = xnew
    
    while abs(diff) > epsilon and n < Nitermax:
        xnew = g(xold)
        diff = xnew - xold
        xold = xnew
        n += 1
    return xnew

print(PointFixe(g7, 1, 10**(-6), 50000))
