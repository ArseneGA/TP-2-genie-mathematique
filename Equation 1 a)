from math import *

def g1plus(x):
    return sqrt(sqrt(9 - 3*x))

def PointFixe(g, xo, epsilon, Nitermax ):
    xold = xo
    n = 1
    xnew = g(xold)
    diff = xnew - xold
    xold = xnew
    
    while abs(diff) > epsilon and n < Nitermax:
        xnew = g(xold)
        diff = xnew - xold
        xold = xnew
        n += 1
    return xnew, n

print(PointFixe(g1, -3, 10**(-3), 50000))
