import math as m
def g2un(x):
    return m.acos((x+2)/3)

def PointFixe(g, xo, epsilon, Nitermax ):
    xold = xo
    n = 1
    xnew = g(xold)
    diff = xnew - xold
    xold = xnew
    
    while abs(diff) > epsilon and n < Nitermax:
        xnew = g(xold)
        diff = xnew - xold
        xold = xnew
        n += 1
    return xnew

print(PointFixe(g2un, 0.5, 10**(-10), 50000))
