import math as m

def g4plus(x):
    return m.log(10 + x)

def PointFixe(g, xo, epsilon, Nitermax ):
    xold = xo
    n = 1
    xnew = g(xold)
    diff = xnew - xold
    xold = xnew
    
    while abs(diff) > epsilon and n < Nitermax:
        xnew = g(xold)
        diff = xnew - xold
        xold = xnew
        n += 1
    return xnew

print(PointFixe(g4plus, -8, 10**(-10), 50000))


