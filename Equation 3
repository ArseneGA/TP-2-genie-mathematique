from math import *

def g1(x):
    return log(7/x, e)

def PointFixe(g, xo, epsilon, Nitermax ):
    xold = xo
    n = 1
    xnew = g(xold)
    diff = xnew - xold
    xold = xnew
    
    while abs(diff) > epsilon and n < Nitermax:
        xnew = g(xold)
        diff = xnew - xold
        xold = xnew
        n += 1
    return xnew

x = PointFixe(g1, 1, 10**(-10), 100000)
print(PointFixe(g1, 1, 10**(-10), 100000))
print(x)
